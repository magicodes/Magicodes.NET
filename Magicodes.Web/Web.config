    </assemblyBinding>
  </runtime>
  <appSettings>
    <!--关于Owin配置，请参考此链接：http://www.asp.net/aspnet/overview/owin-and-katana/owin-startup-class-detection -->
    <!--请不要禁用 OWIN 启动发现-->
    <!--<add key="owin:appStartup" value="Magicodes.StartupSignalR,Magicodes.StartupConfigAuth" />-->
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
</configuration>